---
export interface Props {
  videoId: string;
  videoTitle?: string;
}

const {videoId, videoTitle = "Video"} = Astro.props;
---

<lite-youtube videoid={videoId} nocookie="true" videotitle={videoTitle} params="color=white&rel=0">
</lite-youtube>

<script type="module">
  import "/assets/js/lite-youtube.js";
</script>
